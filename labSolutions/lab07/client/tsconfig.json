{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es5",
        "noImplicitAny": true,
        "sourceMap": true,
        "allowUnreachableCode": false,
        "forceConsistentCasingInFileNames": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "noEmitOnError": true,
        "noFallthroughCasesInSwitch": true,
        "lib": [
            "es5",
            "dom",
            "es2015.symbol",
            "es2015.promise"
        ]
    }
}
