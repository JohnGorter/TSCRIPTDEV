{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es6",
        "noImplicitReturns": true,
        "noFallthroughCasesInSwitch": true,
        "strict": true,
        "noEmitOnError": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "forceConsistentCasingInFileNames": true,
        "sourceMap": true,
        "experimentalDecorators": true,
        "jsx": "react",
        "jsxFactory": "typedHtml.createElement",
        "types": [
            "express"
        ]
    }
}
