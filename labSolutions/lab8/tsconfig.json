{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es6",
        "noImplicitAny": true,
        "sourceMap": true,
        "noImplicitThis": true,
        "moduleResolution": "node",
        "noEmitOnError": false,
        "noImplicitReturns": true,
        "noUnusedLocals": true,
        "noUnusedParameters": false,
        "forceConsistentCasingInFileNames": true,
        "experimentalDecorators": true
    }
}